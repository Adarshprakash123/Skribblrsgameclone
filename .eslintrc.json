{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-plusplus": "off",
        "no-param-reassign": ["error", {"props": true, "ignorePropertyModificationsFor": ["socket", "start", "end"] }],
        "no-console": "off"
    }
}
